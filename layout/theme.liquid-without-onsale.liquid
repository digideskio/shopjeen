</head>

<body>
  {% case template %} 
{% when 'index' %}
    <body id="page-index">
{% else %}
    <body>
{% endcase %}
  {% if settings.search-check %}    
       
        {% endif %}  
{% if settings.show_topbar %}
     
<div id="banner">
    <div class="row">
      
<ul class="inline-list">
	
      
<li><a href="/cart" ><img src="{{ 'cart.png' | asset_url }}"/>MY TOTE ({{ cart.item_count }}) <!-- /.details --></a>
 
  	<ul>
      <li>{% include 'cartdrop' %}</li>	
    
  		</ul>
     
  </li>    
  <li><a href="page/customer-care">CUSTOMER CARE</a></li> 
              <li><a href="/account">LOGIN</a></li>
  

             
            </ul>
         
      <div id="search">     
 <form action="/search" method="get">
<input type="text" name="q" value="" />
<input type="image" src="{{ 'btn-search.png' | asset_url }}" class="search-button" name="search-button" id="seek" />
</form>   
      </div> 
      
    </div>
</div>
{% endif %}
      
<div class="clear"></div>

<div id="head">
    <div id="logo" class="row">
        <h1 class="seven columns" style="border: 0;">
            {% if settings.use_logo_image %}
                <a href="/"><img src="{{ 'logo.png' | asset_url }}" alt="{{ shop.name }}"/></a>
                {% else %}
                <a href="/">{{ shop.name }}</a>
            {% endif %}
        </h1>

        <div class="cart five columns">
            <ul>
             
                {% if shop.customer_accounts_enabled %}
                    {% if customer  %}
                        <li><i class="icon-user"></i><a href="/account">My Account</a></li>
                        <li><i class="icon-signout"></i>{{ "Logout" | customer_logout_link }}</li>
                    {% else %}
                        <li><i class="icon-signin"></i>{{ "Sign in" | customer_login_link }}</li>
                        <li><i class="icon-plus"></i>{{ 'Register' | customer_register_link }}</li>
                    {% endif %}
                {% endif %}
                    <li>
                        <i class="icon-shopping-cart"></i><a href="/cart">My Cart</a>
                    </li>
            </ul>
        </div>     
    </div>


    <div class="row">
        <ul class="nav-bar">
            {% if settings.home-link %}<li><a href="/"><i class="icon-home"></i></a></li>{% endif %}
            {% for link in linklists.main-menu.links %}
                {% capture child_list_handle %}{{ link.title | handleize }}{% endcapture %}
                    {% if linklists[child_list_handle] and linklists[child_list_handle].links.size > 0 %}
                        <li class="has-flyout">{{ link.title | link_to: link.url }}
                        <a href="#" class="flyout-toggle"><span> </span></a>
                            <ul class="flyout">
                                {% for l in linklists[child_list_handle].links %}
                                    <li><a href="{{ l.url }}">{{ l.title }}</a></li>
                                {% endfor %}
                            </ul>
                        </li>
                        {% else %}
                        <li>{{ link.title | link_to: link.url }}</li>
                    {% endif %}
            {% endfor %}
          </li>
        </ul>
    </div>
    
<script type="text/javascript">
$(".nav-bar").addClass("").before('<div id="menu">â˜°</div>');  
$("#menu").click(function(){
    $(".nav-bar").toggle();
});
  


  
$(window).resize(function(){ if(window.innerWidth > 768) { $(".nav-bar").removeAttr("style"); } });
});
</script>

    

      
      <div class="row">
        <div id="three-banners">
          {{ 'sub_nav_ship.png' | asset_url | img_tag }}
          <a href="/collection/clearance">{{ 'sub_nav_clearance.gif' | asset_url | img_tag }}</a>
          <a href="mailto:info@shopjeen.com">{{ 'sub_nav_mail.png' | asset_url | img_tag }}</a>
           
      </div>
      </div>
    
        {% if template != "index" %}
            <div id="content" class="row">
               
                {% if template == 'list-collections' %}
                    {% include 'collection-listing' %}
                {% else %}
                    {{ content_for_layout }}
                {% endif %}
            </div>
        {% else %}
            {{ content_for_layout }}
        {% endif %}
 
<div id="footer">
    <div class="row">
        <ul id="big-footer">
            
            <div class="zero columns">
                <h4>COMPANY</h4>
			{% for link in linklists.footer.links limit: 4 %}                
                   <li>{{ link.title | link_to: link.url }}</li>
                {% endfor %}</div>
           
          <li class="zero columns">
                <h4>CUSTOMER CARE</h4>
                <ul class="no-bullet zero columns" style="margin-left:0px; padding-left: 0;">
              {% for link in linklists.customer-care.links limit: 5 %}                
                   <li>{{ link.title | link_to: link.url }}</li>
                {% endfor %}
                </ul>
                
            </li>
            <li class="zero columns">
                <h4>FIND US ON</h4>
                {% if settings.show_facebook %}<a href="{{ settings.facebook_url }}"><img src="{{ 'Facebook.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_twitter %}<a href="http://www.twitter.com/{{ settings.twittername }}"><img src="{{ 'Twitter.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_pinterest %} <a href="{{ settings.pinterest_url }}"><img src="{{ 'Pinterest.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_tumblr %} <a href="{{ settings.tumblr_url }}"><img src="{{ 'Tumblr.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_flickr %} <a href="{{ settings.flickr_url }}"><img src="{{ 'Flickr.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_youtube %} <a href="{{ settings.youtube_url }}"><img src="{{ 'YouTube.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_googleplus %} <a href="{{ settings.googleplus_url }}"><img src="{{ 'GooglePlus.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_vimeo %} <a href="{{ settings.vimeo_url }}"><img src="{{ 'Vimeo.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_blogger %} <a href="{{ settings.blogger_url }}"><img src="{{ 'Blogger.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_linkedin %} <a href="{{ settings.linkedin_url }}"><img src="{{ 'LinkedIn.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_lastfm %} <a href="{{ settings.lastfm_url }}"><img src="{{ 'Last.fm.png' | asset_url }}"></a>{% endif %}
                {% if settings.show_instagram %} <a href="{{ settings.instagram_url }}"><img src="{{ 'Instagram.png' | asset_url }}"></a>{% endif %}
            </li>  
          <li class="thirteen columns">
          
             
              {% include 'mailing' %}
           
            </li>
          
        </ul>
    </div>
    <div id="end" class="row">
        <div id="close" class="six columns">
            {% if settings.american_express %}<img src="{{ 'amex.png' | asset_url }}"/>{% endif %}
            {% if settings.dankort %}<img src="{{ 'dankort.png' | asset_url }}"/>{% endif %}
            {% if settings.diners %}<img src="{{ 'diners.png' | asset_url }}"/>{% endif %}
            {% if settings.discover %}<img src="{{ 'discover.png' | asset_url }}"/>{% endif %}
            {% if settings.forbru %}<img src="{{ 'forbru.png' | asset_url }}"/>{% endif %}
            {% if settings.google %}<img src="{{ 'google.png' | asset_url }}">{% endif %}
            {% if settings.jcb %}<img src="{{ 'jcb.png' | asset_url }}">{% endif %}
            {% if settings.maestro %}<img src="{{ 'maestro.png' | asset_url }}"/>{% endif %}
            {% if settings.mastercard %}<img src="{{ 'mastercard.png' | asset_url }}"/>{% endif %}
            {% if settings.paypal %}<img src="{{ 'paypa.png' | asset_url }}"/>{% endif %}  
            {% if settings.solo %}<img src="{{ 'solo.png' | asset_url }}"/>{% endif %}  
            {% if settings.visa %}<img src="{{ 'visa.png' | asset_url }}"/>{% endif %}
        </div>
       
    </div>   
</div> 


<div class="share-buttons share-buttons-tab" data-buttons="twitter,facebook,google,email,pinterest,tumblr,polyvore" data-style="big" data-counter="true" data-hover="false" data-promo-callout="false" data-float="left"></div>

</body>
</html>
