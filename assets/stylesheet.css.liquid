/* #Base Styles
================================================== */
* {
	vertical-align: baseline;
	font-weight: inherit;
	font-family: inherit;
	font-style: inherit;
	font-size: 100%;
	border: 0 none;
	outline: 0;
	padding: 0;
	margin: 0;
	}
.sorry{margin-left:2px; height:50px;}
#button{color:#fff;}
html {padding: 0;
  margin: 0; font-size: 62.5%; height:100%; }
    body {
  padding: 0;
  margin: 0;
  height:100%;
		background: transparent;
    font-family: 'Titillium Web', sans-serif;
		font-size: {{ settings.font-size }};
    text-shadow: 1px 1px 1px rgba(0,0,0,0.004);
    font-style: normal;
    font-weight: normal;
    line-height: 1;
    position: relative; 
    color: {{ settings.text_color }};
    text-rendering: optimizeLegibile;
 }

/* #Typography
================================================== */

    body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, p, blockquote, th, td { margin: 0; padding: 0; direction: ltr; }

	h1, h2, h3, h4, h5, h6 {
		color: #181818;
		font-family: 'Titillium Web', sans-serif;
		font-weight: 100;
    text-transform: {{ settings.headings-text-transform }};
        }
	h1 a, h2 a, h3 a, h4 a, h5 a, h6 a { font-weight: inherit; }
	h1 { font-size: {{ settings.h1-size }}; margin-bottom: 10px;}
	h2 { font-size: {{ settings.h2-size }}; margin-bottom: 10px; color:#ec008c; }
	h3 { font-size: {{ settings.h3-size }}; margin-bottom: 8px; }
	h4 { font-size: {{ settings.h4-size }}; margin-bottom: 5px; }
	h5 { font-size: {{ settings.h5-size }}; margin-bottom: 5px; }
	h6 { font-size: {{ settings.h6-size }}; margin-bottom: 5px; }
    hr { border: solid #ddd; border-width: 1px 0 0; clear: both; margin: 22px 0 21px; height: 0; }
    
    .subheader { line-height: 1.3; color: #6f6f6f; font-weight: 300; margin-bottom: 17px; }

	p { margin: 0 0 17px 0; line-height: 1.6; font-size: {{ settings.font-size }}; }
	p img { margin: 0; }
    p.lead { font-size: 17.5px; line-height: 1.6; margin-bottom: 17px; }

    em, i { font-style: italic; line-height: inherit; }
    strong, b { font-weight: bold; line-height: inherit; }
    small { font-size: 90%; line-height: inherit; }
    code { font-weight: bold; background: #ffff99; }
    
    b {font-weight: bold;}
    i { font-style: italic; }

/*	Blockquotes  */
	blockquote, blockquote p { font-size: 17px; line-height: 24px; color: #777; font-style: italic; }
	blockquote { margin: 0 0 20px; padding: 9px 20px 0 19px; border-left: 1px solid #ddd; }
	blockquote cite { display: block; font-size: 12px; color: #555; }
	blockquote cite:before { content: "\2014 \0020"; }
	blockquote cite a, blockquote cite a:visited, blockquote cite a:visited { color: #555; }


/* #Links
================================================== */

	a, a:visited { color:{{ settings.link_color}}; outline: 0; text-decoration: none; }
  a img { border: 0; outline: 0; border-style: none; text-decoration: none; text-shadow: 0 none; }
a:hover, a:focus { color:{{ settings.link_hover_color }}; text-decoration: {{ settings.link-decoration }}; }
	p a, p a:visited { line-height: inherit; }

/* Misc ---------------------- */

    .left { float: left; }
    .right { float: right; }
    .text-left { text-align: left; text-transform: {{ settings.headings-text-transform }}; padding-bottom: 10px; margin-bottom: 20px;}
    .text-right { text-align: right; }
    .text-center { text-transform: {{ settings.headings-text-transform }}; text-align: center; padding-bottom: 10px; margin-bottom: 20px; }
    .hide { display: none; }
    .hide-override { display: none !important; }
    .highlight { background: #ffff99; }
    #googlemap img, object, embed { max-width: none; }
    #map_canvas embed { max-width: none; }
    #map_canvas img { max-width: none; }
    #map_canvas object { max-width: none; }

