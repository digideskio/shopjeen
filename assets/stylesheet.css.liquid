/* =============== */
/* = Footer = */
/* =============== */
#top{ z-index:9999; position:fixed; margin-left:960px; right:20px;
    bottom:15px;
 }
.bottomLine{background-color:#fff;}
#footer { background-color:#E5f0fd;bottom:0; padding-bottom: 0px; height:250px; margin-top:10px; }
.left { float: left; }
.left h4 { display: inline; margin-right: 10px; }
ul.list { list-style-type: none; }
ul.list h4 { display: inline-block; font-family: 'Muli', sans-serif;}
ul.list li { display: inline-block; }
#footer-form{ position:absolute; }
#footer-form form{height:40px; width: 200px; margin-top:0px; }
#signup{margin-top:-20px;}
#check{margin-top:20px; margin-left:600px; position:absolute;}
.news.button{border:0px; border-top:0px;width:50px; font-style:bold; height:20px; background-color:transparent; color:#dd9ef7; font-size:10px;}
.news.button:hover{background-color:transparent;color:#dd9ef7; }
ul#big-footer {margin-left:20px;width:100%; font-family: 'Muli', sans-serif;bottom:0; float: left; border-top: 1px solid {{ settings.footer-top-border }}; margin-bottom: 0px; border-bottom: 0px solid {{ settings.footer-bottom-border }}; padding: 30px 0 10px; list-style-type: none; }
ul#big-footer li { margin-bottom: 5px; font-size:10px;  }
ul#big-footer li img { width: auto; margin: 0 5px 5px 0; }
ul#big-footer { list-style-type: none; color: {{ settings.footer-text-color }}; }
ul#big-footer a { color: {{ settings.footer-link-color }}; }
ul#big-footer a:hover { color: {{ settings.footer-link-hover-color }}; }
ul#big-footer h4 { font-weight: bold! important; margin-bottom: 10px; color: #ec008c; font-family: 'Muli', sans-serif; }
#mc-embedded-subscribe { border-top:0px;margin-left:250px; margin-top:200px; line-height: 1.4; margin-right:0px;  }
#mce-EMAIL {font-size:9px; color:#dd9ef7; float: left; width:250px; margin-left:50px; margin-top:90px; height:25px; position:absolute;  }
#mc_embed_signup{color:#dd9ef7; }
#subscribe{margin-top:90px; margin-left:240px; float:left; background-color:transparent; height:auto; border-top:0px; width:60px; color:#dd9ef7;}
#end{ margin-left:355px;  width:200px; position:absolute; margin-top:90px; }
#end h4 { font-weight: bold;
  margin-bottom: 10px;
  color: #ec008c;
  font-family: 'Muli',sans-serif;}
#end img{width:40px;}

#close img { width:40px; margin: 0 10px 10px 0; }
#close-right { text-align: right;  line-height: 12px; color: {{ settings.footer-text-color }}; }
#close-right a {font-size:12px; color: {{ settings.footer-link-color }}; }
#close-right a:hover { color: {{ settings.footer-link-hover-color }}; }

